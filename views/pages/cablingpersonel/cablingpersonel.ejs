<!-- Modal -->
<div class="modal fade bd-example-modal-lg" id="personelModal" tabindex="-1" role="dialog"
  aria-labelledby="exampleModalLabel" aria-hidden="true" data-keyboard="false" data-backdrop="static">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Edit Departments</h5>
      </div>
      <div class="modal-body">
        <div class="row mb-2">
          <div class="col-lg-12">
            <div class="card-body">
              <div class="row mb-2">
                <div class="col-md-12">
                  <label class="form-label" for="systemidmodal">System ID</label>
                  <input id="systemidmodal" name="systemidmodal" type="text" class="form-control"
                    placeholder="Enter new department" aria-label="fullname" aria-describedby="basic-addon1" disabled />
                </div>
              </div>
              <div class="row">
                <div class="col-md-12">
                  <label class="form-label" for="personelnamemodal">Personel Name</label>
                  <input id="personelnamemodal" name="personelnamemodal" type="text" class="form-control"
                    placeholder="Enter new personel name" aria-label="personelnamemodal"
                    aria-describedby="basic-addon1" />
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="d-none d-sm-inline-block btn btn-sm btn-outline-primary shadow-sm"
          data-dismiss="modal">
          <i class="fa fa-times" aria-hidden="true"></i> Close
        </button>
        <button type="button" id="saveBtn" class="d-none d-sm-inline-block btn btn-sm btn-outline-primary shadow-sm">
          <i class="fa fa-check" aria-hidden="true"></i> Save
        </button>
      </div>
    </div>
  </div>
</div>

<!-- PCreated by Heading -->
<div class="d-sm-flex align-items-center justify-content-between">
  <div class="col-12">
    <div class="row">
      <div class="col-md-6">
        <h1 class="page-title">Cabling Personel</h1>
      </div>
      <div class="col-md-6">
        <div class="d-sm-flex align-items-center justify-content-between">
          <h1 class="page-title">Batch Upload</h1>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- Content Row -->
<div class="row mb-2">
  <div class="col-lg-6">
    <div class="card">
      <div class="card-body">
        <div class="row">
          <div class="col-md-8">
            <div class="input-group">
              <input class="form-control text-center custom-input" placeholder="Select/Input Personel"
                name="personelname" id="personelname" />
              <div class="input-group-append">
                <span class="input-group-text">Personel</span>
              </div>
            </div>

          </div>
          <div class="col-md-4 text-center">
            <button id="addBtn" name="addBtn"
              class="cw-7rem custom-btn shadow-sm">
              <i class="fa fa-plus" aria-hidden="true"></i>
              Add
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-6">
    <div class="card">
      <div class="card-body">

        <div class="row">
          <div class="col-md-8">
            <input type="file" name="excelfile" id="excelfile"
              class="form-control btn btn-inventory-clr shadow-sm w-100" />
          </div>
          <div class="col-md-4 text-center">
            <button id="uploadBtn" name="uploadBtn"
              class="cw-7rem custom-btn shadow-sm">
              <i class="fa fa-arrow-up" aria-hidden="true"></i>
              Upload
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- DataTales Example -->
<div class="card shadow table-size-personel">
  <div class="progress progress-sm mb-2">
